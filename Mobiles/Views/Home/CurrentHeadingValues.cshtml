@model IEnumerable<Mobiles.Data.Models.SpecifictionHeadingValue>

@{
    ViewBag.Title = "CurrentHeadingValues";
}

<h2>CurrentHeadingValues</h2>

<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Value)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Heading_Id)
        </th>
      
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Value)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Heading_Id)
        </td>
    </tr>
}

</table>
